#include<bits/stdc++.h>

using namespace std;

int main() {
    int a, b;
    char n;
    cin >> a >> n >> b;
    if (n == '+'){
        cout << a + b;
    }
    else if (n == '-'){
        cout << a - b;
    }
    else if (n == '*'){
        cout << a * b;
    }
    else if (n == '/'){
        cout << a / b;
    }
}
